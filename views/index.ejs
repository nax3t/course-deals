    <section id="portfolio" class="section-bg">
      <div class="container">

        <header class="section-header">
          <h3 class="section-title mb-0">Suggested Courses</h3>
          <p><small>(Udemy compensates me for any purchases made from the links below)</small></p>
          <h4 class="text-center">What would you like to learn?</h4>
        </header>

        <div class="row">
          <div class="col-lg-12">
            <ul id="portfolio-flters">
              <li data-filter="*" class="filter-active"><button class="btn btn-outline-primary btn-primary">Everything</button></li>
              <% categories.forEach(cat => { %>
                <li data-filter=".filter-<%= cat.name %>"><button class="btn btn-outline-primary"><%= cat.formal %></button></li>
              <% }); %>
            </ul>
          </div>
        </div>

        <div class="row portfolio-container">

					<% var count = 0; %>
          <% courses.forEach(course => { %>
          <% count = count === 3 ? 0 : count; %>
      		<div class="col-lg-4 col-md-6 portfolio-item <%= course.categories ? course.categories.split(' ').map(el=>'filter-'+el).join(' ') : 'null' %>" data-wow-delay="0.<%= count %>s">
            <div class="card">
              <div class="card-body">
                <div class="card-title text-center" style="height:35px;">
                  <a href="<%= course.affiliateUrl %>"><%= course.title %></a>
                </div>
              </div>
        		  <div class="portfolio-wrap">  
        		    <img src="<%= course.thumbnailUrl.replace('240x135', '480x270'); %>" class="img-fluid" alt="<%= course.title %>">
        		    <div class="portfolio-info">
        		      <h5 class="text-white">
                    <%= course.rating %> rating
                  </h5>
        		      <div>
        		        <a href="<%= course.affiliateUrl %>" class="link-details" title="View Course">
                      <i class="ion ion-android-open"></i>
                    </a>
                    <% if (currentUser && currentUser.admin) { %>
                    <a href="/courses/<%= course.id %>/edit" class="link-details" title="Edit Course">
                      <i class="ion ion-android-create"></i>
                    </a>
                    <a href="/courses/<%= course.id %>/delete" class="link-details" title="Delete Course">
                      <i class="ion ion-android-delete"></i>
                    </a>
                    <% } %>
        		      </div>
        		    </div>
        		  </div>
            </div>
      		</div>
      		<% count++; %>
      	  <% }); %>

        </div>

      </div>
    </section><!-- #portfolio -->

